@media print {
    .hbs *, .hbs *:before, .hbs *:after {
        background: transparent !important;
        color: #000 !important;
        box-shadow: none !important;
        text-shadow: none !important
    }

    .hbs a, .hbs a:visited {
        text-decoration: underline
    }

    .hbs a[href^="#"]:after, .hbs a[href^="javascript:"]:after {
        content: ""
    }

    .hbs pre, .hbs blockquote {
        border: 1px solid #999;
        page-break-inside: avoid
    }

    .hbs thead {
        display: table-header-group
    }

    .hbs tr, .hbs img {
        page-break-inside: avoid
    }

    .hbs img {
        max-width: 100% !important
    }

    .hbs p, .hbs h2, .hbs h3 {
        orphans: 3;
        widows: 3
    }

    .hbs h2, .hbs h3 {
        page-break-after: avoid
    }

    .hbs select {
        background: #fff !important
    }

    .hbs .navbar {
        display: none
    }

