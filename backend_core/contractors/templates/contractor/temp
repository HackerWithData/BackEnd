.hbs table {
    background-color: transparent
}

.hbs caption {
    padding-top: 8px;
    padding-bottom: 8px;
    color: #666666;
    text-align: left
}

.hbs th {
    text-align: left
}

.hbs .table {
    width: 100%;
    max-width: 100%;
    margin-bottom: 20px
}

.hbs .table > thead > tr > th, .hbs .table > tbody > tr > th, .hbs .table > tfoot > tr > th, .hbs .table > thead > tr > td, .hbs .table > tbody > tr > td, .hbs .table > tfoot > tr > td {
    padding: 8px;
    line-height: 1.42857143;
    vertical-align: top;
    border-top: 1px solid #ddd
}

.hbs .table > thead > tr > th {
    vertical-align: bottom;
    border-bottom: 2px solid #ddd
}

.hbs .table > caption + thead > tr:first-child > th, .hbs .table > colgroup + thead > tr:first-child > th, .hbs .table > thead:first-child > tr:first-child > th, .hbs .table > caption + thead > tr:first-child > td, .hbs .table > colgroup + thead > tr:first-child > td, .hbs .table > thead:first-child > tr:first-child > td {
    border-top: 0
}

